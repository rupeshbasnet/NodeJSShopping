{{# each products }}
	<div class="row">
	{{# each this }}
	  <div class="col-sm-6 col-md-4">
	    <div class="thumbnail">
	      <img src="{{ this.imagePath }}" class = "image-responsive">
	      <div class="caption">
	        <h3>{{ this.title }}</h3>
	        <div>
	        	<div class = "clearfix">
	        		<div class = "amount pull-left">${{ this.price }}</div>
	        		<a href="#" class="btn btn-default pull-right" role="button">Add to Cart</a>
	        	</div>
	        </div>
	      </div>
	    </div>
	  </div>
	{{/each}}
	</div>
{{/each}}